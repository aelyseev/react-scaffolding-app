{
  "name": "react-scaffolding-app",
  "version": "1.0.0",
  "description": "React, Redux, React Router scaffolfing application. React Bootstrap as view components library",
  "dependencies": {
    "es6-promise": "~4.1.1",
    "gsap": "~1.19.1",
    "invariant": "~2.2.2",
    "lodash": "~4.17.2",
    "moment": "~2.18.1",
    "normalize.css": "~7.0.0",
    "prop-types": "~15.5.10",
    "react": "~16.0.0",
    "react-bootstrap": "~0.31.3",
    "react-dom": "~16.0.0",
    "react-redux": "~5.0.0",
    "react-router": "~4.1.1",
    "react-router-dom": "~4.1.1",
    "redux": "~3.7.1",
    "redux-actions": "^2.2.1",
    "redux-form": "~6.8.0",
    "redux-promise": "~0.5.3",
    "redux-thunk": "~2.2.0",
    "reselect": "~3.0.0",
    "whatwg-fetch": "~2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "~7.1.2",
    "axios": "~0.16.2",
    "babel-cli": "~6.24.0",
    "babel-core": "~6.25.0",
    "babel-eslint": "~7.2.1",
    "babel-loader": "~7.1.1",
    "babel-plugin-lodash": "~3.2.11",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-es2015": "~6.24.0",
    "babel-preset-flow": "~6.23.0",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "babel-register": "~6.24.0",
    "baggage-loader": "~1.0.0",
    "btoa": "~1.1.2",
    "chunk-manifest-webpack-plugin": "1.1.0",
    "clean-webpack-plugin": "~0.1.14",
    "concurrently": "~3.5.0",
    "copy-webpack-plugin": "~4.2.3",
    "cross-env": "~5.0.1",
    "css-loader": "~0.28.4",
    "enzyme": "~2.9.1",
    "enzyme-to-json": "~1.5.1",
    "eslint": "~3.18.0",
    "eslint-config-airbnb": "~13.0.0",
    "eslint-import-resolver-webpack": "~0.8.1",
    "eslint-loader": "~1.6.1",
    "eslint-plugin-flowtype": "~2.30.3",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~2.2.3",
    "eslint-plugin-react": "~6.8.0",
    "extract-text-webpack-plugin": "~3.0.0",
    "file-loader": "~0.11.2",
    "flow-babel-webpack-plugin": "~1.1.0",
    "flow-bin": "~0.51.0",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy": "~1.16.2",
    "image-webpack-loader": "^3.4.2",
    "jest-cli": "~20.0.4",
    "json-loader": "^0.5.7",
    "lodash-webpack-plugin": "~0.11.3",
    "node-sass": "~4.5.2",
    "nodemon": "~1.11.0",
    "postcss-loader": "~2.0.6",
    "pre-commit": "~1.2.2",
    "raw-loader": "~0.5.1",
    "react-hot-loader": "~3.0.0-beta.6",
    "react-test-renderer": "~15.6.1",
    "resolve-url-loader": "~2.1.0",
    "sass-loader": "~6.0.3",
    "style-loader": "~0.18.2",
    "stylefmt": "~6.0.0",
    "stylelint": "~7.12.0",
    "stylelint-config-standard": "~16.0.0",
    "stylelint-order": "~0.5.0",
    "url-loader": "~0.5.7",
    "webpack": "~3.1.0",
    "webpack-dev-server": "~2.5.1",
    "webpack-merge": "~4.1.0",
    "yaml-loader": "^0.5.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "deploy": "****",
    "eslint": "eslint \"server/**/*.js\" \"app/**/*.js\" \"app/**/*.jsx\" --fix --cache --cache-location \"./.cache/\"",
    "flow": "flow",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "stylelint": "stylelint \"app/**/*.scss\"",
    "stylelint:fix": "stylefmt --recursive 'app/**/*.scss'",
    "test": "jest",
    "test:watch": "cross-env NODE_ENV=development jest --watch"
  },
  "pre-commit": {
    "silent": true,
    "run": [
      "eslint",
      "stylelint"
    ]
  },
  "babel": {
    "babelrc": false,
    "presets": [
      [
        "es2015",
        {
          "loose": true,
          "modules": false
        }
      ],
      "stage-0",
      "react"
    ],
    "plugins": [
      "lodash",
      "transform-flow-strip-types",
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "engines": {
    "node": "^8.7.0",
    "npm": "~3.10.0"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "DEBUG": false
    },
    "roots": [
      "<rootDir>/app"
    ],
    "modulePaths": [
      "<rootDir>/app"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!koa)"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "<rootDir>/.fixtures/style.js",
      "^.+\\.path$": "<rootDir>/.fixtures/path.js"
    },
    "testRegex": ".*test.jsx?$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/.fixtures/tests-setup.js"
    ]
  },
  "author": "Aleksey Elyseev",
  "license": "UNLICENSED"
}
